    <!--BEGIN OF Information resource-->

    <Style x:Key="InformationResource" TargetType="Path">
        <Setter Property="Data" Value="M 0,0 H 60 V40 H 0 Z"/>
        <Setter Property="Fill" Value="PaleTurquoise"/>
        <Setter Property="Fill">
            <Setter.Value>
                <ImageBrush ImageSource='C:\Users\Vanya\Desktop\Новая модель ресурсов\DiagramDesigner\Resources\Images\ResourceImages\information.png'/>
            </Setter.Value>
        </Setter>
        <Setter Property="Stretch" Value="Fill"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Tag" Value="Information"/>

    </Style>


    <Style x:Key="InformationResource_DragThumb" TargetType="Path" BasedOn="{StaticResource InformationResource}">
        <Setter Property="IsHitTestVisible" Value="true"/>

