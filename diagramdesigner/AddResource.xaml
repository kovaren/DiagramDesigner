<Window x:Class="DiagramDesigner.AddResource"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавление ресурса" Height="289" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <ListBox x:Name="listBox" Margin="10,10,10,41" FontSize="20" SelectionMode="Multiple">
            <ListBoxItem Content="Информационный ресурс"/>
            <ListBoxItem Content="Финансовый ресурс"/>
            <ListBoxItem Content="Трудовой ресурс"/>
            <ListBoxItem Content="Оборудование"/>
            <ListBoxItem Content="Товар"/>
            <ListBoxItem Content="Услуга"/>
        </ListBox>
        <Button x:Name="OKButton" Content="OK" Margin="0,0,105,10" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="90" Click="OKButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" Margin="0,0,10,10" HorizontalAlignment="Right" Width="90" Height="26" VerticalAlignment="Bottom" IsCancel="True"/>
    </Grid>
</Window>
